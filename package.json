{
  "name": "appointment-reservations-for-doctor-restful-apis",
  "version": "1.0.0",
  "description": "A reservation app using express, node and typescript",
  "main": "src/app.ts",
  "engines": {
    "node": ">=16",
    "npm": "*"
  },
  "scripts": {
    "start": "node ./build/app.js",
    "dev": "nodemon",
    "build": "rimraf ./build && tsc",
    "lint": "eslint ./src --ignore-path .gitignore --ignore-pattern internals/scripts --ext .ts,.tsx .",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:watch": "npm run lint --watch",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Drashti-Patel/Appointment-reservations-for-Doctor-Restful-APIs.git"
  },
  "author": "Minahil Imtiaz <minaahilimtiaz@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Drashti-Patel/Appointment-reservations-for-Doctor-Restful-APIs/issues"
  },
  "homepage": "https://github.com/Drashti-Patel/Appointment-reservations-for-Doctor-Restful-APIs#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "firebase": "^10.6.0",
    "firebase-admin": "^11.11.0",
    "http-status": "^1.0.1",
    "jose": "^4.9.3",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.21",
    "@types/node-jose": "^1.1.10",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.20",
    "prettier": "^3.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1"
  }
}
